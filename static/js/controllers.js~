var myApp = angular.module('myApp', []);
myApp.controller('myAppCtrl', function ($scope, $http) {
    $scope.items = [
      {'name': 'Item1', 'value': '1'},
      {'name': 'Item2', 'value': '2'},
      {'name': 'Item3', 'value': '3'}
  ];

  var promise = $http.get('/test');
  promise.then(function(data, status){
    console.log(data, status);
   // $scope.releases = data.data.results.sort();
    console.log("I am an alert box!");
  }, function(){
    console.log("Error ");
  });

});
